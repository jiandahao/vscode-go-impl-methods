{
  "name": "vscode-go-impl-methods",
  "publisher": "jiandahao",
  "displayName": "Tools for generating and extracting Golang interface",
  "description": "Automatically implement and extract interface for Golang by using code generation",
  "version": "0.0.0-development",
  "icon": "img/icon.png",
  "license": "SEE LICENSE IN LICENSE",
  "keywords": [
    "go",
    "golang",
    "interface",
    "method",
    "stubs",
    "generate"
  ],
  "author": {
    "name": "Dahao.Jian",
    "email": "jiandahao@gmail.com"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:go",
    "onCommand:goimpl.ImplementInterfaceMethods",
    "onCommand:goimpl.ExtractInterface"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "goimpl.ImplementInterfaceMethods",
        "title": "Implement Interface Methods",
        "category": "Go"
      },
      {
        "command": "goimpl.ExtractInterface",
        "title": "Extract Interface",
        "category": "Go"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/chai": "^4.2.14",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.51.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "chai": "^4.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "semantic-release": "^17.3.0",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "vsce": "^1.81.1",
    "vscode-test": "^1.4.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@types/lodash": "^4.14.165",
    "child_process": "^1.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
